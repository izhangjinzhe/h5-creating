{
    "root": true,
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:vue/essential",
        "eslint:recommended",
        "@vue/standard"
    ],
    "parserOptions": {
        "parser": "@babel/eslint-parser"
    },
    "rules": {
        "vue/multi-word-component-names" : "off",
        "vue/no-mutating-props": "off",
        "vue/no-useless-template-attributes": "off"
    }
}
